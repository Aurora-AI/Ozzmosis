    "lint": "eslint .",
    "test:survival": "vitest run tests/survival"
  },
  "dependencies": {
    "@aurora/aurora-conductor": "*",
    "@elysiajs/node": "^1.0.0",
    "elysia": "^1.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^9.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  }
}
