    "smoke": "npm run build && node ./scripts/smoke.mjs"
  },
  "dependencies": {
    "@elysiajs/node": "^1.0.0",
    "elysia": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.16"
  }
}
